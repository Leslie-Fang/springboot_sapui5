<mvc:View
        controllerName="sap.ui.demo.walkthrough.controller.InvoiceList"
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc">
    <List
            id="customerList"
            class="sapUiResponsiveMargin"
            width="auto"
            items="{
                 path : 'car>/Customers'
            }" >
        <items>
            <ObjectListItem
                    title="{car>Id} x {car>Name}" />
        </items>
    </List>
</mvc:View>